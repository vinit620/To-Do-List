<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>To-Do List</title>
</head>

<body>
    <h1> <%= day %> </h1>

    <ul>
        <% tasks.forEach(task => { %>
            <li> <%= task %> </li>
        <% }) %>
    </ul>
</body>

</html>